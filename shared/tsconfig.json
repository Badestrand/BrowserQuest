{
  "compilerOptions": {
  	"target": "es6",
	"lib": ["dom", "dom.iterable", "esnext"],
	"allowJs": true,
	"allowSyntheticDefaultImports": true,
	"skipLibCheck": true,
	"esModuleInterop": true,
	"strict": false,
	"forceConsistentCasingInFileNames": true,
	"moduleResolution": "node",
	"resolveJsonModule": true,
	"isolatedModules": true,
	"noEmit": false,
	"jsx": "react"
  },
  "include": ["js", "../shared"]
}